{
  "repository": "https://github.com/listrophy/lazy_ncaa_elm",
  "license": "Apache-2.0",
  "devDependencies": {
    "elm-css": "^0.6.0",
    "elm-live": "^2.7.4"
  },
  "scripts": {
    "build": "mkdir -p dist && elm-css src/Stylesheets.elm && elm-make src/Main.elm --output=index.js",
    "deploy": "gh-pages-deploy",
    "clean-source": "rimraf elm-stuff src tests *.sh"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "cname": "lazybracket.com",
    "prep": [
      "build"
    ],
    "post": [
      "clean-source"
    ],
    "noprompt": true
  }
}
